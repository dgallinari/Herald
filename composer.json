{
    "name": "heraldofarms/herald",
    "description": "The messenger for your app to convey messages or notifications",
    "keywords": ["herald", "messages", "notifications", "integrations"],
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Passarelli",
            "email": "passarelli.gabriel@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },"autoload": {
    "psr-4": {
            "HeraldOfArms\\Herald\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HeraldOfArms\\Herald\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
